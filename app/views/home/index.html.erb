
<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<ul>
  <%= @data[2][1].each do |data| %>

    <li><%= data['name'] %></li>
    <li><%= data['phone'] %></li>
    <li><%= data['location'] %></li>
    <li>
      <% data['categories'].each_with_index  do |category, index| %>
        <%= category.first %>
        <%= "|" unless index == data['categories'].count - 1 %>
      <% end %>
    </li>
    <li><%= data['is_closed'] %></li>
    <li><%= data['location'] %></li>
    <li><%= data['rating'] %></li>

  <% end %>
</ul>
